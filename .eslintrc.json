{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "require": "readonly",
    "module": "readonly",
    "process": "readonly",
    "__dirname": "readonly",
    "__filename": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "indent": [ "warn", 4, { "SwitchCase": 1 } ],
    "no-unused-vars": 0
  },
  "settings": {}
}